[
  {
    "label": "open_lazygit",
    "command": "lazygit",
    "env": {
        "EDITOR": "zed --wait"
    },
    "use_new_terminal": false,
    "allow_concurrent_runs": false,
    "reveal": "always",
    "hide": "on_success"
  },
  {
    "label": "open_vs_code",
    "command": "code $ZED_WORKTREE_ROOT $ZED_RELATIVE_FILE",
    "use_new_terminal": false,
    "allow_concurrent_runs": false,
    "reveal": "never",
    "hide": "always"
  }
]
